\CommentTok{\# bitcoin }

\NormalTok{BTC\_TREND\_BR}\OtherTok{=} \FunctionTok{plot}\NormalTok{(btc\_hist}\SpecialCharTok{$}\NormalTok{timestamp[}\DecValTok{2}\SpecialCharTok{:}\DecValTok{3200}\NormalTok{],logdiff,}
     \AttributeTok{type=}\StringTok{\textquotesingle{}l\textquotesingle{}}\NormalTok{,}\AttributeTok{col=}\StringTok{\textquotesingle{}red\textquotesingle{}}\NormalTok{,}
     \AttributeTok{xlab =} \StringTok{"time (t)"}\NormalTok{,}
     \AttributeTok{ylab =} \StringTok{"Y(t)"}\NormalTok{,}
     \AttributeTok{main =} \StringTok{"Trend signal"}\NormalTok{)}
\end{Highlighting}
\end{Shaded}

